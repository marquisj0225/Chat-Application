{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "dist",
      "sourceMap": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "types": [
        "node",
        "jest"
      ],
    },
    "include": [
      "*.ts",
      "**/*.ts",
      "routes/*.ts",
      "routes/*.json"
    ],
    "exclude": [
      "node_modules"
    ],
  }
  